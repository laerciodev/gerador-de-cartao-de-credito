{
  "name": "Gera dados do cartão de crédito",
  "version": "1.0.0",
  "description": "Adiciona dados do cartão de crédito ao formulário de checkout",
  "default_locale": "pt_BR",
  "action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "script.js"
  },
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "64": "./icons/icon64.png",
    "128": "./icons/icon128.png"
  }
}